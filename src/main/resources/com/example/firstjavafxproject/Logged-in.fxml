<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="499.0" prefWidth="916.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstjavafxproject.LoggedInController">
   <children>
      <Label fx:id="label_leaving_from" layoutX="31.0" layoutY="62.0" text="Leaving From ">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="label_going_go" layoutX="242.0" layoutY="62.0" text="Going To">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="button_logout" layoutX="701.0" layoutY="16.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="189.0" style="-fx-background-color: #FED700;" text="Logout ">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="cb_leaving_from" layoutX="31.0" layoutY="98.0" prefWidth="150.0" />
      <ChoiceBox fx:id="cb_going_to" layoutX="242.0" layoutY="98.0" prefWidth="150.0" />
      <Label fx:id="label_departing" layoutX="452.0" layoutY="62.0" text="Departing">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="label_returning" layoutX="646.0" layoutY="62.0" text="Returning">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <DatePicker fx:id="dp_departing" layoutX="433.0" layoutY="98.0" promptText="Select a departing date  " />
      <DatePicker fx:id="dp_returning" layoutX="634.0" layoutY="98.0" promptText="Select a returning date  " />
      <Button fx:id="button_search" layoutX="330.0" layoutY="156.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="189.0" text="Search" />
      <TableView fx:id="tb_flights" layoutX="10.0" layoutY="212.0" prefHeight="200.0" prefWidth="894.0">
        <columns>
          <TableColumn fx:id="cl_departure" prefWidth="106.0" text="Departure" />
            <TableColumn fx:id="cl_arrival" prefWidth="105.0" text="Arrival" />
            <TableColumn fx:id="cl_departure_time" prefWidth="116.0" text="Departure Time" />
            <TableColumn fx:id="cl_duration" prefWidth="95.0" text="Duration " />
            <TableColumn fx:id="cl_price" prefWidth="84.5" text="Price" />
            <TableColumn fx:id="cl_departing_date" prefWidth="148.0" text="Departing Date" />
            <TableColumn fx:id="cl_return_date" prefWidth="129.0" text="Return Date" />
            <TableColumn fx:id="cl_flight_id" prefWidth="109.5" text="Flight_id" />
        </columns>
      </TableView>
      <Button fx:id="button_check_profile" layoutX="707.0" layoutY="448.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="189.0" text="Check your profile" />
      <Label fx:id="label_welcome" layoutX="14.0" layoutY="20.0" prefHeight="23.0" prefWidth="189.0" text="Welcome">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
